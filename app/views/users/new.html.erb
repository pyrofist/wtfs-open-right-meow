<%= form_for @user do |f| %>
  <div>
    <%= f.text_field :email,
        placeholder: "Email" %>
  </div>
  <div>
    <%= f.text_field :username,
        placeholder: "Username" %>
  </div>
  <div>
    <%= f.password_field :password,
        placeholder: "Password" %>
  </div>
  <div>
    <%= f.password_field :password_confirmation,
        placeholder: "Password Confirmation" %>
  </div>
  <div>
    <%= f.submit "Sign Up" %>
  </div>
<% end %>
